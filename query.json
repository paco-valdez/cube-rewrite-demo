{
  "measures": [
    "EstimatedOpportunity.hcpCount",
    "EstimatedOpportunity.socCount",
    "EstimatedOpportunity.opportunityValue"
  ],
  "dimensions": ["ZipcodeLocation.zipCode"],
  "order": {
    "ZipcodeLocation.count": "desc"
  },
  "filters": [
